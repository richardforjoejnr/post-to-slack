{
  "name": "@richardforjoesky/post-to-slack",
  "version": "1.3.0",
  "description": "module to post to slack",
  "license": "UNLICENSED",
  "main": "./dist/src/index.j",
  "private": false,
  "files": [
    "src",
    "bin"
  ],
  "bin": {
    "send-slack-message": "./dist/bin/slack.js"
  },
  "scripts": {
    "test": "vitest --run --reporter verbose --coverage --globals",
    "build": "babel src --out-dir dist/src && babel bin --out-dir dist/bin",
    "report": "node ./src/slack.js send-slack-message --template './sample/testreport_template' --data './src/slack-message-tool/sample/slack_data.json'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/richardforjoesky/post-to-slack.git"
  },
  "keywords": [
    "slack"
  ],
  "author": "Richard Forjoe",
  "bugs": {
    "url": "https://github.com/richardforjoesky/post-to-slack/issues"
  },
  "homepage": "https://github.com/richardforjoesky/post-to-slack#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@slack/web-api": "^6.8.1",
    "colors": "^1.4.0",
    "commander": "^10.0.1",
    "dotenv": "^16.0.3",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.3",
    "@vitest/coverage-c8": "^0.31.0",
    "babel-cli": "^6.26.0",
    "cz-conventional-changelog": "^3.3.0",
    "vitest": "^0.31.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
