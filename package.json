{
  "name": "@richardforjoesky/post-to-slack",
  "version": "2.0.0",
  "description": "module to post to slack",
  "license": "UNLICENSED",
  "main": "./src/index.js",
  "private": false,
  "files": [
    "src"
  ],
  "bin": {
    "send-slack-message": "./src/index.js"
  },
  "scripts": {
    "test": "vitest --run --reporter verbose --coverage --globals",
    "report": "node ./src/index.js send-slack-message --template './sample/testreport_template' --data './src/slack-message-tool/sample/slack_data.json'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/richardforjoesky/post-to-slack.git"
  },
  "keywords": [
    "slack"
  ],
  "author": "Richard Forjoe",
  "bugs": {
    "url": "https://github.com/richardforjoesky/post-to-slack/issues"
  },
  "homepage": "https://github.com/richardforjoesky/post-to-slack#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@slack/web-api": "^6.8.1",
    "colors": "^1.4.0",
    "commander": "^10.0.1",
    "dotenv": "^16.0.3",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "semantic-release": "^21.0.2",
    "@vitest/coverage-c8": "^0.31.0",
    "cz-conventional-changelog": "^3.3.0",
    "vitest": "^0.31.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "assets": ["src/**/*.js", "src/**/*.json"]
        }
      ]
    ]
  }
}
